 });\r\n  }\r\n  render() {\r\n    if (this.state.loading) {\r\n      return <h1> loading...</h1>;\r\n    }\r\n    const {\r\n      name,\r\n      animal,\r\n      breed,\r\n      location,\r\n      description,\r\n      media,\r\n      showModal\r\n    } = this.state;\r\n\r\n    return (\r\n      <div className=\"details\">\r\n        <Carousel media={media} />\r\n        <div>\r\n          <h1>{name}</h1>\r\n          <h2>\r\n            {animal} - {breed} - {location}\r\n          </h2>\r\n          <button onClick={this.toggleModal}>Adopt {name}</button>\r\n          <p>{description}</p>\r\n          {showModal ? (\r\n            <Modal>\r\n              <h1>Would you like to adopt {name}</h1>\r\n              <div className=\"buttons\">\r\n                <button onClick={this.toggleModal}>Yes</button>\r\n                <button onClick={this.toggleModal}>Definitely Yes</button>\r\n              </div>\r\n            </Modal>\r\n          ) : null}\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default Details;\r\n"},"lineCount":null}},"hash":"9959f3caf03113da0b9355677fd92d39","cacheData":{"env":{"API_KEY":"9c87a2b1484e97978d421c52b992bb56","API_SECRET":"8e408189dde0166598332a278f8353e7"}}}